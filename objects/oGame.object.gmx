                    } else {
                        if (global.mobileBuild) {
                            menuTouchControls();
                        } else {
                            menuToggleFullscreen();
                        }
                    }
                    break;
                }
                case 2: {
                    if (global.html5Build) {
                        if (global.mobileBuild) {
                            menuLanguage();
                        } else {
                            menuToggleFullscreen();
                        }
                    } else {
                        if (global.mobileBuild) {
                            menuLanguage();
                        } else {
                            menuLanguage();
                        }
                    }
                    break;
                }
                case 3: {
                    if (global.html5Build) {
                        if (global.mobileBuild) {
                            menuDie();
                        } else {
                            menuLanguage();
                        }
                    } else {
                        if (global.mobileBuild) {
                            menuDie();
                        } else {
                            menuDie();
                        }
                    }
                    break;
                }
                case 4: {
                    if (global.html5Build) {
                        if (global.mobileBuild) {
                            instance_activate_all();
                            audio_stop_all();
                            game_restart();
                        } else {
                            menuDie();
                        }
                    } else {
                        if (global.mobileBuild) {
                            instance_activate_all();
                            audio_stop_all();
                            game_restart();
                        } else {
                            quitGame();
                        }
                    }
                    break;
                }
                case 5: {
                    if (global.browserBuild) {
                        instance_activate_all();
                        audio_stop_all();
                        game_restart();
                    } else {
                        quitGame();
                    }
                    break;
                }
            }
        }
        configSave()
    }
}
